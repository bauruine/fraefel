<% if params[:pdf_type].present? %>

  <% if params[:pdf_type] == "invoice" %>
    <%= render "cargo_lists/pdf_partials/invoice.pdf.erb" %>
  <% elsif params[:pdf_type] == "report" %>
    <%= render "cargo_lists/pdf_partials/report.pdf.erb" %>
  <% elsif params[:pdf_type] == "report_external" %>
    <%= render "cargo_lists/pdf_partials/report_external.pdf.erb" %>
  <% elsif params[:pdf_type] == "report_lebert" %>
    <%= render "cargo_lists/pdf_partials/report_lebert.pdf.erb" %>
  <% elsif params[:pdf_type] == "report_controll" %>
    <%= render "cargo_lists/pdf_partials/report_controll.pdf.erb" %>
  <% elsif params[:pdf_type] == "delivery_receipt" %>
    <%= render "cargo_lists/pdf_partials/delivery_receipt.pdf.erb" %>
  <% elsif params[:pdf_type] == "pick_up_receipt" %>
    <%= render "cargo_lists/pdf_partials/pick_up_receipt.pdf.erb"%>
  <% end %>

<% end %>