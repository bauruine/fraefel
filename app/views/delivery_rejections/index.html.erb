<header class="resource_header margin_bottom_big clearfix">
  <h1 class="floatLeft">Konflikte / Annahme Verweigerung</h1>
    <%= link_to("Konflikt erfassen", new_delivery_rejection_path(), :class => "floatRight no_text_decoration blue_link_button m-r_5px") %>
</header>

<section class="filter_box two_third">
  <header class="filter_box_header"><h4>&nbsp;</h4></header>
  <section class="filter_box_content clearfix">
    <table>
      <thead>
        <tr>
          <th>Konflikt NR</th>
          <th>Grund</th>
          <th>Status</th>
          <th>Anz. Positionen</th>
          <th>Kunde</th>
          <th>Erfasst von</th>
          <th>Erstellt am</th>
        </tr>
      </thead>
      <tbody>
        <%= render @delivery_rejections %>
      </tbody>
      <tfoot>
      </tfoot>
    </table>
  </section>
</section>

<section class="filter_box one_third">
  <header class="filter_box_header clearfix">
    <h4 class="floatLeft">Filter anwenden</h4>
    <%= link_to("Gehe zu", "#", {"data-type" => "tab", "data-warp" => "goto", :class => "floatRight no_text_decoration link_button"}) %>
    <%= link_to("Filter", "#", {"data-type" => "tab", "data-warp" => "filter", :class => "floatRight no_text_decoration link_button m-r_5px"}) %>
    </header>
  <section class="filter_box_content clearfix">
    <div class="box-padder">
    </div>
  </section>
</section>