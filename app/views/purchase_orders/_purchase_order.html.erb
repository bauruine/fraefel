<tr class="purchase_order">
  <td>
    <% if purchase_order.priority_level != 1 %>
      <i class="icon-prio<%= purchase_order.priority_level %>"></i>
    <% end %>
    <a href="/purchase_orders/<%= purchase_order.id %>"><%= purchase_order.baan_id %></a>
  </td>
  <td>
    <div class="btn-group">
      <%= purchase_order.html_content.code.html_safe %>
    </div>
  </td>
  <td><%= purchase_order.calculation.total_pallets %></td>
  <td><%= purchase_order.shipping_address.consignee_full %></td>
  <td><%= purchase_order.shipping_route.name %></td>
  <td><%= purchase_order.purchase_positions.first.zip_location.title %></td>
  <td><%= purchase_order.delivery_date.to_formatted_s(:swiss_date) %></td>
</tr>

