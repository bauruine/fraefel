<fieldset>
  <legend>Versand</legend>
  <div class="control-group">
    <%= f.label(:shipping_route_id_equals, "Tour", :class => "control-label") %>
    <div class="controls">
      <%= f.collection_select(:shipping_route_id_equals, @shipping_routes, :id, :name, {:include_blank => true}, {"data-foo" => "shipping_routes"}) %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label(:warehouse_number_equals, "Versandlager", :class => "control-label") %>
    <div class="controls">
      <%= f.text_field(:warehouse_number_equals) %>
    </div>
  </div>
</fieldset>