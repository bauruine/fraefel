<fieldset>
  <legend>Versand</legend>
  
  <div class="control-group">
    <%= f.label :delivery_date, "Versand Datum", :class => "control-label" %>
    <div class="controls">
      <%= f.text_field :delivery_date, :class => "date_picker" %>
    </div>
  </div>
  
  <div class="control-group">
    <%= f.label(:shipping_route_id, "Tour", :class => "control-label") %>
    <div class="controls">
      <%= f.collection_select(:shipping_route_id, @shipping_routes, :id, :name, {}, {}) %>
    </div>
  </div>
  <div class="control-group">
    <%= f.label(:warehouse_number, "Versandlager", :class => "control-label") %>
    <div class="controls">
      <%= f.text_field(:warehouse_number) %>
    </div>
  </div>
  
</fieldset>